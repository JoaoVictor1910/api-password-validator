{
  "name": "api-password-validator",
  "version": "0.1.0",
  "description": "Api para validar uma senha de acordo com as regras configuradas",
  "contributors": [
    {
      "name": "Jo√£o Freire",
      "url": "https://github.com/JoaoVictor1910"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoaoVictor1910/api-password-validator.git"
  },
  "bugs": {
    "url": "https://github.com/JoaoVictor1910/api-password-validator/issues"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env ENV=test TZ=America/Sao_Paulo jest --passWithNoTests --runInBand --no-cache",
    "test:watch": "pnpm t -- --watch --silent",
    "test:cov": "pnpm t -- --coverage",
    "test:staged": "pnpm t -- --findRelatedTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm t -- --config ./jest-e2e.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "9.4.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/event-emitter": "1.4.1",
    "@nestjs/microservices": "9.4.2",
    "@nestjs/platform-express": "9.4.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.0.3",
    "jest-mock-extended": "3.0.4",
    "jsonwebtoken": "9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.1",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.2",
    "@protobuf-ts/protoc": "2.8.3",
    "@types/express": "4.17.13",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.11.18",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "git-commit-msg-linter": "4.9.4",
    "jest": "29.3.1",
    "jest-cucumber": "3.0.1",
    "lint-staged": "13.2.0",
    "mockdate": "3.0.5",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "timekeeper": "2.2.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.7.0",
    "ts-proto": "1.145.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.7.4"
  },
  "volta": {
    "node": "18.16.0"
  }
}
